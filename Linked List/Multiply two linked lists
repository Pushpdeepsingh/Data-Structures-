def multiplyTwoList(l1, l2):
    num1=0
    num2=0
    while(l1 or l2):

        if(l1):
            num1 =num1*10 + l1.data
            l1=l1.next
      
        if(l2):
            num2 =num2*10 + l2.data;
            l2=l2.next;
      
  
    return ((num2%1000000007)*(num1%1000000007))%1000000007;
