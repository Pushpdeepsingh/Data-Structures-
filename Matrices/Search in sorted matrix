lass Solution:
	def matSearch(self,mat, N, M, X):
	    
	   last_col=M-1
	   for row in range (0,N):
	       if mat[row][last_col]>=X:
	           for j in range (0,M):
	               if(mat[row][j]==X):
	                   return 1
	   return 0
     
     '''
     N = 3, M = 3
mat[][] = 3 30 38 
         44 52 54 
         57 60 69
X = 62'''
